{
    "default": {
        "formateOptions": {
            "snippetInterface": "async-await",
            "resultsDir": "allure-results"
        },
        "paths": [
            "tests/features/*.feature"
        ],
        "publishQuite": true,
        "dryRun": false,
        "require": [
            "tests/steps/*.js",
            "tests/support/*.js"
        ],
        "format": [
            "allure-cucumberjs/reporter"
        ]
    }
}